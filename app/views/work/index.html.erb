<h1>Form#index</h1>
<p>Find me in app/views/form/index.html.erb</p>


<%= form_for @work, url: {action: "create"} do |f| %>
  <%= f.label :Name%>
  <%= f.text_field :name %>
  <br>
  <%= f.label :Year%>
  <%= f.text_field :year %>
  <br>
  <%= f.label :Branch%>
  <%= f.text_field :branch %>
  <br>
  <%= f.label :Percentage%>
  <%= f.text_field :string %>
  <br>
  <%= f.label :SOP%>
  <%= f.text_field :sop %>
  <br>
  <%= f.submit "Apply" %>
<% end %>